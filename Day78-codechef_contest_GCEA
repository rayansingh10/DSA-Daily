https://www.codechef.com/problems/GCEA

#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    cin>>t;
    while(t--){
        int n,x,y;
        cin>>n>>x>>y;
        vector<int> a(n,0);
        for(int i=0;i<n;i++) cin>>a[i];
        int coins=0;
        for(int i=0;i<n;i++){
            if(a[i]*x>y) coins=coins+y;
            else coins=coins+(a[i]*x);
        }
        cout<<coins<<endl;
    }
}
